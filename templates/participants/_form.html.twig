{{form_start(form)}}
<div class="d-flex flex-justify-center flex-align-center">
    <div class="cell-3 p-5 ">
        {{ form_label(form.pseudo) }}
    </div>
    <div class="cell-9 p-5 ">
        {{ form_widget(form.pseudo) }}
    </div>
</div>
<div class="d-flex flex-justify-center flex-align-center">
    <div class="cell-3 p-5 ">
        {{ form_label(form.prenom) }}
    </div>
    <div class="cell-9 p-5 ">
        {{ form_widget(form.prenom) }}
    </div>
</div>
<div class="d-flex flex-justify-center flex-align-center">
    <div class="cell-3 p-5 ">
        {{ form_label(form.nom) }}
    </div>
    <div class="cell-9 p-5 ">
        {{ form_widget(form.nom) }}
    </div>
</div>
<div class="d-flex flex-justify-center flex-align-center">
    <div class="cell-3 p-5 ">
        {{ form_label(form.telephone) }}
    </div>
    <div class="cell-9 p-5 ">
        {{ form_widget(form.telephone) }}
    </div>
</div>

<div class="d-flex flex-justify-center flex-align-center">
    <div class="cell-3 p-5 ">
        {{ form_label(form.email) }}
    </div>
    <div class="cell-9 p-5 ">
        {{ form_widget(form.email) }}
    </div>
</div>

<div class="d-flex flex-justify-center flex-align-center">
    <div class="cell-3 p-5 ">

        {# {{ form_alert(form.password) }} #}
        {{ form_label(form.password) }}
    </div>
    <div class="cell-9 p-5 ">
        {{ form_widget(form.password) }}
    </div>
</div>

<div class="d-flex flex-justify-center flex-align-center">
    <div class="cell-3 p-5 ">
        {{ form_label(form.sitesNoSite) }}
    </div>
    <div class="cell-9 p-5 ">
        {{ form_widget(form.sitesNoSite) }}
    </div>
</div>

</div>
<button class="button success">{{ button_label|default('Save') }}</button>
{{form_end(form)}}